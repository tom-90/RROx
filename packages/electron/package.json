{
  "name": "@rrox/electron",
  "productName": "RailroadsOnline Extended",
  "private": true,
  "version": "2.2.1",
  "description": "Railroads Online Extended is a mod for the game Railroads Online",
  "author": "_tom()",
  "main": ".webpack/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/tom-90/RROx.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development yarn rebuild-native && electron-forge start",
    "debug": "cross-env NODE_ENV=development yarn rebuild-native && electron-forge start --inspect-electron",
    "rebuild-native": "cd ../../ && yarn electron-rebuild -m packages/electron --version 18.2.0",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build": "yarn run make",
    "publish": "electron-forge publish",
    "lint": "eslint src/ --ext .ts,.js,.tsx,.jsx"
  },
  "config": {
    "forge": "./tools/forge/forge.config.js"
  },
